import java.util.Scanner;
/*
 * Cody Henopp
 * 02/18/2020
 * 
 * This program eliminates and duplicates in the array.
 */
public class Exercise7_15 {
	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);

		System.out.println("Enter 10 numbers separated by a space: ");
		int [] numbers = new int[10];
		for (int line = 0; line < numbers.length; line++) {
			numbers[line] = input.nextInt();
		}
		System.out.println("Array without Duplicates: " + eliminateDuplicates(numbers));
	}

	public static int [] eliminateDuplicates(int[] list) {
		int [] duplicate = new int [100];
		for (int line = 0; line < list.length; line++) 
			duplicate[list[line]-'1']++;

		return duplicate;

	}

}
